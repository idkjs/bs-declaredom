let with_flex_css =
    (
      ~style: option(Css_Style.t(Css_Property.flex_item))=?,
      ~css_module: option(Css_Module.t(Css_Property.flex_item))=?,
      value: Html_Node.t([< Html_Node.element(_)]),
    )
    : Html_Node.t([> Html_Node.flex_item]) => {
  let module_name = Css_Module.get_class(~css_module?, ())
  and element: Dom.element = (
    value
    |> Html_Node.to_node
    |> Webapi.Dom.Node.cloneNodeDeep
    |> Webapi.Dom.Element.ofNode
    |> Js.Option.getExn: Dom.element
    /* Get the underlying Dom.element, should be safe since it's an Html_Node.element */
  );

  let _ =
    switch (module_name) {
    | Some(module_name') =>
      /* Append module class name */
      let class_name = Webapi.Dom.Element.className(element);
      Webapi.Dom.Element.setClassName(element) @@
      class_name
      ++ " "
      ++ module_name';
    | _ => ()
    };

  let _ =
    switch (style) {
    | Some(style') =>
      let element_style = Internal.Dom.style(element);

      style'
      |> Js.Dict.entries
      |> Js.Array.forEach(((key, value)) =>
           Internal.Dom.set_style(
             element_style,
             key,
             Css_Property.show(value),
           )
         );
    | _ => ()
    };

  Internal.Node.to_flex_item(value);
};
