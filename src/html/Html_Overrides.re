/* Use this instead of Css_Style.t for tree-shaking */
type style('a) = Js.Dict.t(Css_Property.t('a));

module Internal = {
  /* Erases type information */

  module Style = {
    external to_inline:
      style([< Css_Property.display]) => style(Css_Property.inline) =
      "%identity";
    external to_block:
      style([< Css_Property.display]) => style(Css_Property.block) =
      "%identity";

    let display_inline_block = style =>
      style |> Css_Properties.Display.inline_block |> to_block;

    let display_inline_block' = style =>
      style |> Css_Properties.Display.inline_block |> to_inline;
  };

  module CssModule = {
    external to_inline:
      Css_Module.t([< Css_Property.display]) =>
      Css_Module.t(Css_Property.inline) =
      "%identity";

    external to_block:
      Css_Module.t([< Css_Property.display]) =>
      Css_Module.t(Css_Property.block) =
      "%identity";
  };

  module Node = {
    external to_any:
      array(Html_Node.t([< Html_Node.content(_)])) =>
      array(Html_Node.t([< Html_Node.content(_)])) =
      "%identity";

    external to_flex_item:
      Html_Node.t([< Html_Node.content(_)]) =>
      Html_Node.t([> Html_Node.flex_item]) =
      "%identity";
  };

  module Dom = {
    type style;
    [@bs.get] external style: Dom.element => style = "style";
    [@bs.set_index] external set_style: (style, string, string) => unit;
  };
};

